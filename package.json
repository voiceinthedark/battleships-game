{
  "name": "battleship-game",
  "version": "0.2.35",
  "description": "Battleship game in vanilla javascript",
  "main": "index.js",
  "homepage": "https://github.com/voiceinthedark/battleships-game",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "dev": "webpack serve --config webpack.dev.js",
    "deploy": "gh-pages -d dist",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watchall",
    "lint": "npx eslint .",
    "format": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "prelint": "npm run format:check",
    "postlint": "npm run format",
    "jsdoc": "jsdoc -c jsdoc.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/voiceinthedark/battleships-game.git"
  },
  "keywords": [
    "javascript",
    "vanilla-javascript",
    "css",
    "webpack",
    "battleship-game",
    "battleship",
    "jest",
    "tdd"
  ],
  "author": "voiceinthedark",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/voiceinthedark/battleships-game/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@eslint/js": "^9.31.0",
    "@types/jest": "^30.0.0",
    "babel-jest": "^30.0.5",
    "css-loader": "^7.1.2",
    "eslint": "^9.31.0",
    "gh-pages": "^6.3.0",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^30.0.4",
    "jsdoc": "^4.0.4",
    "prettier": "3.6.2",
    "style-loader": "^4.0.0",
    "svg-inline-loader": "^0.8.2",
    "webpack": "^5.100.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "@stdlib/string-format": "^0.2.2",
    "cli-color": "^2.0.4"
  }
}
